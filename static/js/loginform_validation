(function() {
    $('form > input').keyup(function() {

        var empty = false;
        $('form > input').each(function() {
            if ($(this).val() == '') {
                empty = true;
            }
        });

        if (empty) {
            $('#btnLogin').attr('disabled', 'disabled');
        } else {
            $('#btnLogin').removeAttr('disabled');
        }
    });
})()